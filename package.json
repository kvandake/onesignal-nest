{
  "name": "onesignal-api-client-nest",
  "version": "1.0.7",
  "description": "OneSignal API wrapper for the Server Rest API made to work with the Nest framework.",
  "author": {
    "email": "kvandake@gmail.com",
    "name": "kvandake"
  },
  "license": "MIT",
  "keywords": [
    "onesignal",
    "typescript",
    "nest",
    "nestjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kvandake/onesignal-nest"
  },
  "bugs": {
    "url": "https://github.com/kvandake/onesignal-nest/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.6.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@nestjs/common": "^6.6.3",
    "@nestjs/core": "^6.6.3",
    "onesignal-api-client-core": "^1.1.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "files": [
    "lib/**/*"
  ]
}
